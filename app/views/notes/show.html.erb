<h2>Note: <%= @note.title %></h2>

<br/>

<table>

  <tr>
    <td>ID</td>
    <td><%= @note.display_id %></td>
  </tr>

  <tr>
    <td>Tittel</td>
    <td><%= @note.title %></td>
  </tr>

  <tr>
    <td>Besetning</td>
    <td><%= @note.voice %></td>
